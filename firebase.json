{
  "hosting": {
    "site": "resilientpsj",
    "public": "app",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "run": {
          "serviceId": "psjdash",
          "region": "us-east1"
        }
      }
    ]
  }
}
